{
  "main":
          [
            {
              "tag":"div",
              "class":"title"
            },
            {
              "tag":"div",
              "class":"description"
            },
            {
              "tag":"div",
              "class":"current_question"
            }
          ],

  "question":
  {
    "tag":"div",
    "class":"question",
    "inner":
            [
              {
                "tag":"div",
                "inner":
                        [
                          {
                            "tag":"span",
                            "inner":
                                    [
                                      {
                                        "tag":"span",
                                        "inner":"%question%"
                                      },
                                      "%nr%"
                                    ]
                          },
                          {
                            "tag":"span",
                            "inner":"%text%"
                          }
                        ]
              },
              {
                "tag":"from",
                "onsubmit":"%onsubmit%",
                "inner":
                [
                  {
                    "tag":"div",
                    "class":"answers"
                  },
                  {
                    "tag":"div",
                    "class":"button",
                    "inner":
                    {
                      "tag":"input",
                      "type":"submit",
                      "value":"%caption%"
                    }
                  }
                ]
              }
            ]
  },

  "answer":
  {
    "tag": "div",
    "class": "answer",
    "inner":
    [
      {
        "tag": "div",
        "inner":
        [
          {
            "tag": "div",
            "class": "input"
          },
          {
            "tag": "div",
            "class": "text",
            "inner":
            {
              "tag": "label",
              "inner": "%text%",
              "for": "%id%"
            }
          }
        ]
      }
    ]
  }
}